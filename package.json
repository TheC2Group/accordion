{
  "name": "c2-accordion",
  "version": "2.8.0",
  "description": "Accessible accordion using CSS transition to expand and contract.",
  "jsnext:main": "accordion.js",
  "main": "cjs/accordion.js",
  "export": "Accordion",
  "scripts": {
    "iife": "rollup -c rollup-iife.config.js",
    "cjs": "rollup -c rollup-cjs.config.js",
    "umd": "rollup -c rollup-umd.config.js",
    "build": "npm run iife && npm run cjs && npm run umd",
    "bundle-example": "browserify test/js/index.js -o test/bundle/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:TheC2Group/accordion.git"
  },
  "keywords": [
    "accordion"
  ],
  "author": "The C2 Group",
  "license": "MIT",
  "copyright": "(c) The C2 Group (c2experience.com)",
  "bugs": {
    "url": "https://github.com/TheC2Group/accordion/issues"
  },
  "homepage": "https://github.com/TheC2Group/accordion",
  "dependencies": {
    "jquery": ">=3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-preset-es2015-rollup": "^3.0.0",
    "browserify": "^16.2.3",
    "acorn": "^6.0.0",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0"
  }
